{
  "name": "astronomy-svg",
  "version": "0.1.8",
  "description": "Generate astronomy themed scalable vector graphics (SVG) images easily.",
  "keywords": [
    "astronomy",
    "svg",
    "earth",
    "mars",
    "jupiter",
    "sun",
    "moon",
    "javascript",
    "universe",
    "saturn"
  ],
  "homepage": "https://alexandreribeiro.github.io/astronomy-svg",
  "bugs": {
    "url": "https://github.com/alexandreribeiro/astronomy-svg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexandreribeiro/astronomy-svg.git"
  },
  "license": "MIT",
  "author": "Alexandre Ribeiro",
  "type": "module",
  "main": "index.js",
  "files": [
    "/dist",
    "/lib",
    "/demo",
    "README.md",
    "LICENSE",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "vite build",
    "watch": "vite",
    "test": "vitest --run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare-docs": "npm run build && rm -rf docs && mkdir docs && mkdir docs/dist && cp -r dist/* docs/dist/ && cp README.md docs/readme.md && cp -r demo docs/ && cp -r images docs/"
  },
  "dependencies": {
    "astronomy-js": "^0.1.0",
    "luxon": "^3.6.1"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "vite": "^6.3.5",
    "vite-plugin-banner": "^0.8.1",
    "vitest": "^3.1.4"
  }
}
